meta {
  name: Fresher - Submit (multipart)
  type: http
}
request {
  method: POST
  url: {{baseUrl}}/api/freshers/submit
  headers {
    Accept: application/json
  }
  body: formdata {
    name: Alice
    email: alice@example.com
    phone: 9999999999
    qualification: B.Tech
    skills: JAVA,SPRING
    # Attach a local file path for resume if desired
    # resume: file(C:/path/to/resume.pdf)
  }
}
